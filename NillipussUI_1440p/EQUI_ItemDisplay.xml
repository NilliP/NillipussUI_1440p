<?xml version="1.0" encoding="us-ascii"?>
<XML ID="EQInterfaceDefinitionLanguage">
  <Schema xmlns="EverQuestData" xmlns:dt="EverQuestDataTypes" />
  <STMLbox item="IDW_ItemDescription">
    <ScreenID>ItemDescription</ScreenID>
    <DrawTemplate>WDT_RoundedNoTitle</DrawTemplate>
    <RelativePosition>true</RelativePosition>
    <Style_VScroll>true</Style_VScroll>
    <AutoStretch>true</AutoStretch>
    <LeftAnchorOffset>0</LeftAnchorOffset>
    <TopAnchorOffset>0</TopAnchorOffset>
    <RightAnchorOffset>0</RightAnchorOffset>
    <BottomAnchorOffset>0</BottomAnchorOffset>
    <RightAnchorToLeft>false</RightAnchorToLeft>
    <BottomAnchorToTop>false</BottomAnchorToTop>
    <LeftAnchorToLeft>true</LeftAnchorToLeft>
    <TopAnchorToTop>true</TopAnchorToTop>
    <Style_Border>false</Style_Border>
    <Text>Test text</Text>
    <Font>5</Font>
  </STMLbox>
  <Button item="IDW_IconButton">
    <ScreenID>IconButton</ScreenID>
    <!--<Font>3</Font>-->
    <RelativePosition>true</RelativePosition>
    <AutoStretch>true</AutoStretch>
    <LeftAnchorOffset>100</LeftAnchorOffset>
    <TopAnchorOffset>12</TopAnchorOffset>
    <RightAnchorOffset>12</RightAnchorOffset>
    <BottomAnchorOffset>100</BottomAnchorOffset>
    <RightAnchorToLeft>false</RightAnchorToLeft>
    <BottomAnchorToTop>true</BottomAnchorToTop>
    <LeftAnchorToLeft>false</LeftAnchorToLeft>
    <TopAnchorToTop>true</TopAnchorToTop>
    <Style_VScroll>false</Style_VScroll>
    <Style_HScroll>false</Style_HScroll>
    <Style_Transparent>false</Style_Transparent>
    <!--<TooltipReference/>-->
    <Style_Checkbox>false</Style_Checkbox>
    <!--<RadioGroup/>-->
    <ButtonDrawTemplate>
      <Normal>RedIconBackground</Normal>
      <NormalDecal>BuffIcons</NormalDecal>
    </ButtonDrawTemplate>
    <SoundPressed />
    <SoundUp />
    <SoundFlyby />
    <DecalOffset>
      <X>4</X>
      <Y>4</Y>
    </DecalOffset>
    <DecalSize>
      <CX>80</CX>
      <CY>80</CY>
    </DecalSize>
  </Button>
  <Screen item="ItemDisplayWindow">
    <!--<ScreenID/>-->
    <!--<Font/>-->
    <RelativePosition>true</RelativePosition>
    <Location>
      <X>0</X>
      <Y>80</Y>
    </Location>
    <Size>
      <CX>800</CX>
      <CY>380</CY>
    </Size>
    <Style_VScroll>false</Style_VScroll>
    <Style_HScroll>false</Style_HScroll>
    <Style_Transparent>false</Style_Transparent>
    <DrawTemplate>WDT_RoundedNoTitle</DrawTemplate>
    <Style_Titlebar>true</Style_Titlebar>
    <Style_Closebox>true</Style_Closebox>
    <Style_Minimizebox>true</Style_Minimizebox>
    <Style_Border>true</Style_Border>
    <Style_Sizable>true</Style_Sizable>
    <Pieces>IDW_ItemDescription</Pieces>
    <Pieces>IDW_IconButton</Pieces>
    <Font>5</Font>
  </Screen>
</XML>